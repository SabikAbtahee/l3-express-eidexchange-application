<%- include('header') %>
    <div class="mb-4">
        <a href="/manage-event/<%= eventId %>" class="text-red-500 hover:text-red-700 ml-2"> <i
                class="fa-solid fa-arrow-left-long"></i> Go back to manage event</a>
    </div>
    <div class="p-6">
        <div class="text-3xl mb-4">All Members wishlist</div>
        <div>
            <% for(let i of list) { %>
                <div class="text-2xl font-bold mb-2">
                    <%= i.name %>
                </div>
                <% for(let j of i.wishlist) { %>
                    <ul>
                        <li>
                            <%= j %>
                        </li>
                    </ul>

                    <% } %>

                        <% if(!i.wishlist?.length) {%>
                            <span>---</span>
                            <% } %>

                                <% } %>
        </div>
    </div>


    <%- include('footer') %>