<%- include('header') %>
    <div class="my-4">
        <a href="/manage-event/<%= eventId %>" class="text-teal-500 hover:text-teal-700 ml-2"> <i
                class="fa-solid fa-arrow-left-long"></i> Go back to manage event</a>
    </div>
    <div class="p-2 overflow-auto">
    <table class="w-full table-auto border-collapse border border-gray-400 responsive-text-md">
        <thead>
            <tr>
                <th class="border border-gray-300 p-2">This member is giving ...</th>
                <th class="border border-gray-300 p-2">a gift to this member</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i of matches) { %>
                <tr>
                    <td class="border border-gray-300 p-2">
                        <%= i.member %>
                    </td>
                    <td class="border border-gray-300 p-2">
                        <%= i.assignee %>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
</div>

    <%- include('footer') %>